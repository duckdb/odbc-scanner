# name: test/sql/oracle/09_timestamp.test
# description: test for TIMESTAMP type with Oracle
# group: [oracle_09_timestamp]

require odbc_scanner

statement ok
SET VARIABLE conn = odbc_connect('ODBCSCANNER_DEBUG_CONN_STRING_ENV_VAR=ODBC_CONN_STRING')

query I
SELECT * FROM odbc_query(getvariable('conn'),
  'SELECT to_timestamp(''2020-12-31 12:34:56.123456789'', ''YYYY-MM-DD HH24:MI:SS.FF9'') FROM dual')
----
2020-12-31 12:34:56.123456

query I
SELECT column_type from (
  DESCRIBE SELECT * FROM odbc_query(getvariable('conn'),
    'SELECT to_timestamp(''2020-12-31 12:34:56.123456789'', ''YYYY-MM-DD HH24:MI:SS.FF9'') FROM dual')
)
----
TIMESTAMP

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP(6)) FROM dual',
  params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP(9)) FROM dual',
  params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 23:58:59.123456

query I
SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP(3)) FROM dual',
  params=row('2020-12-31 23:58:59.123456'::TIMESTAMP))
----
2020-12-31 23:58:59.123

statement ok
SET VARIABLE params1 = odbc_create_params()

statement ok
PREPARE p1 AS SELECT * FROM odbc_query(getvariable('conn'), 'SELECT CAST(? AS TIMESTAMP(9)) FROM dual',
  params_handle=getvariable('params1'))

statement ok
PREPARE p1_bind AS SELECT odbc_bind_params(getvariable('conn'), getvariable('params1'), row(?))

statement ok
EXECUTE p1_bind('2020-12-31 23:58:59.123456'::TIMESTAMP)

query I
EXECUTE p1
----
2020-12-31 23:58:59.123456

statement ok
EXECUTE p1_bind('1970-01-01 00:00:00'::TIMESTAMP)

query I
EXECUTE p1
----
1970-01-01 00:00:00

statement ok
EXECUTE p1_bind('1969-12-31 00:00:00'::TIMESTAMP)

query I
EXECUTE p1
----
1969-12-31 00:00:00

statement ok
EXECUTE p1_bind('0001-01-01 00:00:00.000000'::TIMESTAMP)

query I
EXECUTE p1
----
0001-01-01 00:00:00

statement ok
EXECUTE p1_bind('9999-12-31 23:59:59.999999'::TIMESTAMP)

query I
EXECUTE p1
----
9999-12-31 23:59:59.999999

statement ok
DEALLOCATE p1_bind

statement ok
DEALLOCATE p1
