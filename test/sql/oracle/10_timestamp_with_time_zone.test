statement ok
EXECUTE p2_bind('0001-01-01 00:00:00.000000'::TIMESTAMP)

query I
EXECUTE p2
----
-infinity

statement ok
EXECUTE p2_bind('7166-12-31 23:59:59.999999'::TIMESTAMP)

query I
